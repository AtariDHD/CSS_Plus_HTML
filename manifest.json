{
  "name": "CSS+HTML",
  "description": "Capture styles and markup for element plus children.",
  "version": "0.32",
  "manifest_version": 2,
  "minimum_chrome_version": "18.0",
    "permissions": [
        "tabs", "<all_urls>"
    ],
  "icons": {
     "48": "img/logo_48.png",
     "64": "img/logo_64.png",
     "128": "img/logo_128.png"
  },
  "devtools_page": "devtools.html",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/jquery-1.8.2.min.js", "js/content.js"],
  		"run_at": "document_end"
    }
  ]
}
